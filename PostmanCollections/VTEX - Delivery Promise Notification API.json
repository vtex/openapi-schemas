            "method": "PUT",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"itemId\": \"B\",\n  \"productId\": \"XPTO\",\n  \"availability\": 7,\n  \"promises\": [\n    {\n      \"availability\": 5,\n      \"deliveryChannel\": \"delivery\",\n      \"deliveryZoneIds\": [\n        \"BRA_COUNTRY\"\n      ],\n      \"deliveryInfo\": {\n        \"id\": \"Prime5Days\",\n        \"name\": \"Same Week\",\n        \"deliveryTime\": \"5.00:00:00\"\n      }\n    },\n    {\n      \"availability\": 3,\n      \"deliveryChannel\": \"pickup-in-point\",\n      \"deliveryZoneIds\": [\n        \"BRA_COUNTRY\"\n      ],\n      \"deliveryInfo\": {\n        \"id\": \"Prime2Day\",\n        \"name\": \"Same Day\",\n        \"deliveryTime\": \"1.00:00:00\"\n      }\n    }\n  ]\n}",
