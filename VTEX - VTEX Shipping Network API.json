          "example": {
            "name": "Ltda",
            "accountName": "{{account}}",
            "isOperating": true,
            "defaultUrl": null,
            "district": null,
            "country": "Brasil",
            "complement": null,
            "companyName": "LTDA",
            "cnpj": "00000000000000",
            "city": null,
            "address": null,
            "number": null,
            "postalCode": null,
            "state": null,
            "telephone": null,
            "tradingName": "tradingName"
          }
