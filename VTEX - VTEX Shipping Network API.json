                    "postalCode": null,
                    "state": null,
                    "telephone": null,
                    "tradingName": "tradingName"
                  },
                  "email": "email",
                  "dispatchOrder": {
                    "id": "653560c2-7965-4614-a730-8e45c89bf284",
                    "packageIds": [
                      "6529cd28-6fd1-4b95-8397-5fad0a79d41c"
                    ],
                    "sender": {
                      "cnpj": "00000000000000",
                      "fantasyName": null,
                      "stateRegistration": null,
                      "id": null,
                      "name": "LTDA",
                      "address": {
                        "postalCode": "03475015",
